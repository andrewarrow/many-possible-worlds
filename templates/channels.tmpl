{{template "_header" .}}
<h1 class="entry-title">Channels</h1>

        <ul class="cols">
                <li><a href="/channels">Latest</a></li>
                <li>|</li>
                <li><a href="/channels?sort=vc">View Count</a></li>
        </ul>

<div class="featured">
    {{ range $l := .latest }}
        <ul class="cols">
        <li>
        <p><a href="/channel/{{$l.ChannelId}}"><img border="1" width="176" height="176" src="{{$l.ImageUrl}}"/></a></p>
        </li>
        <li>
           <p>
           <strong>{{$l.ChannelTitle}}</strong>
           published
  <a href="https://www.youtube.com/watch?v={{$l.ExampleVideoId}}">{{ $l.ExampleVideoTitle}}</a>  
           </p>
        </li>
        <li>
           <p>
  {{ deltaAgo $l.ExampleVideoPublishedAt}} ago

           </p>
        </li>
        <li>
                <p> {{$l.SubscriberCount}} subscriber(s).  </p>
                <p> {{$l.ViewCount}} total channel view(s).  </p>
                <p> {{$l.VideoCount}} total channel video(s).  </p>
        </li>
        </ul>
    {{ end }}
</div>

{{template "_footer" .}}
